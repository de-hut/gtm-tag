const log = require('logToConsole');
const copyFromWindow = require('copyFromWindow');
const queryPermission = require('queryPermission');


if (queryPermission('access_globals', 'readwrite', 'inject_script')) {
// Chame data.gtmOnSuccess depois que a tag for concluída.
  const customerCode = data.customerCode;
  const injectScript = require('injectScript');
  const url = 'https://widget.dehut.com.br/widget.js';
  injectScript(url, data.gtmOnSuccess, data.gtmOnFailure, url);
  
  const createElement = copyFromWindow('createElement');
  const body = copyFromWindow('body');
  const tagDehut = createElement('dehut-integration');
  tagDehut.setAttribute('identifier',customerCode);
  body.appendChild(tagDehut);
}
